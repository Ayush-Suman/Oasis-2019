<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:orientation="vertical"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".events.view.fragments.MiscEventsFragment">

    <TextView
            android:text="Misc Events Fragment"
            android:layout_width="185dp"
            android:layout_height="49dp"
            android:id="@+id/textView" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="152dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="201dp" android:textSize="24sp" app:layout_constraintHorizontal_bias="0.708"
            app:layout_constraintBottom_toTopOf="@+id/miscEventRecycler"/>

    <androidx.recyclerview.widget.RecyclerView
            android:layout_width="372dp"
            android:layout_height="576dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="58dp"
            android:layout_marginEnd="20dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="19dp"
            android:layout_marginTop="33dp" app:layout_constraintTop_toBottomOf="@+id/textView"
            android:id="@+id/miscEventRecycler"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"/>

</androidx.constraintlayout.widget.ConstraintLayout>